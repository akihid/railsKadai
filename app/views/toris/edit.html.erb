<h1>編集画面</h1>

<%= form_with(model: @post, local: true , url: tori_path ) do |form| %>

 <%= render partial: "check_error" %>
 
  <div class="post_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit "つぶやく" %>
<% end %>

<%= form_with(model:@post , local: true , url: toris_path ,method: 'get') do |form| %>
  <%= form.hidden_field :content %>
  <%= form.submit  "戻る" ,name: 'back' %>
<% end %>